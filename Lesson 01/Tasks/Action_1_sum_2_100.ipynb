    "# 求2+4+6+...+100的和\n",
    "\n",
    "def sum(n1,n2):   \n",
    "    a = n1\n",
    "    b = n1    \n",
    "    while b < n2:\n",
    "        b = b + 2\n",
    "        a = a + b   \n",
    "    return a\n",
    "\n",
    "c = sum(2,100)\n",
